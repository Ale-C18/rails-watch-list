<h1>List <%= @list.name %></h1>

<% @list.movies.each do |movie| %>
  <%= image_tag movie.poster_url, width: "100px" %>
  <%= movie.title %>
  <div>
    <% movie.bookmarks.each do |element| %>
      <%= element.comment %>
    <% end %>
  </div>
<% end %>

<%= link_to "Back to Lists", lists_path(@list) %>
